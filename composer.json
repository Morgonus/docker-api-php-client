{
    "name": "morgonus/docker-php",
    "license": "MIT",
    "type": "library",
    "description": "A Docker PHP client",
    "autoload": {
        "psr-4": {
            "Docker\\": "src/",
            "Docker\\API\\": "generated/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docker\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "jane-php/open-api-runtime": "4.2.0",
        "jane-php/open-api": "4.2.0",
        "jane-php/json-schema": "4.2.0",
        "amphp/artax": "^3.0",
        "guzzlehttp/psr7": "^1.2",
        "php-http/client-common": "^1.6",
        "php-http/socket-client": "^1.3",
        "php-http/message": "^1.0",
        "symfony/filesystem": "^2.3 || ^3.0 || ^4.0",
        "symfony/process": "^2.3 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "phpstan/phpstan": "^0.12.25",
        "friendsofphp/php-cs-fixer": "2.16.3",
        "amphp/artax": "^3.0",
        "amphp/socket": "^0.10.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-clover build/coverage.xml",
        "lint": "vendor/bin/phpstan analyse --configuration phpstan.neon"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.40-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
